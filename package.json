{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "wifi_manager",
  "title": "WIFI Manager",
  "description": "Quickly turn WIFI on/off",
  "categories": [
    "Productivity",
    "Applications",
    "Developer Tools",
    "System"
  ],
  "icon": "wifi-manager.png",
  "author": "haneenmahdin",
  "license": "MIT",
  "commands": [
    {
      "name": "turn-off-wifi",
      "title": "Turn off WIFI",
      "description": "Turn off the WIFI",
      "mode": "no-view",
      "icon": "wifi_off.png"
    },
    {
      "name": "turn-on-wifi",
      "title": "Turn on WIFI",
      "description": "Turn on the WIFI",
      "mode": "no-view",
      "icon": "wifi_on.png"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}